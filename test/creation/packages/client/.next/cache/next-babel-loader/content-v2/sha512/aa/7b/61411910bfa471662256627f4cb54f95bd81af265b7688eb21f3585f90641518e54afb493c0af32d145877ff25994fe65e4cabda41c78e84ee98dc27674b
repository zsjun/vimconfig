{"ast":null,"code":"import \"antd/lib/message/style\";\nimport _message from \"antd/lib/message\";\nimport axios from 'axios';\nexport var httpProvider = axios.create({\n  baseURL: process.env.SERVER_API_URL,\n  timeout: 60000\n});\nhttpProvider.interceptors.request.use(function (config) {\n  return config;\n}, function () {\n  throw new Error('发起请求出错');\n});\nhttpProvider.interceptors.response.use(function (data) {\n  if (data.status && +data.status === 200 && data.data.status === 'error') {\n    true && _message.error({\n      message: data.data.msg\n    });\n    return null;\n  }\n\n  var res = data.data;\n\n  if (!res.success) {\n    _message.error(res.msg);\n\n    return null;\n  }\n\n  return res.data;\n}, function (err) {\n  if (err && err.response && err.response.status) {\n    var status = err.response.status;\n\n    switch (status) {\n      case 504:\n      case 404:\n        true && _message.error('服务器异常');\n        break;\n\n      default:\n        true && _message.error(err.response && err.response.data && err.response.data.msg || '未知错误!');\n    }\n  }\n\n  return Promise.reject(err);\n});","map":null,"metadata":{},"sourceType":"module"}