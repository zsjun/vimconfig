{"ast":null,"code":"// 标签颜色\nexport var TAG_COLORS = ['magenta', 'volcano', 'orange', 'gold', 'lime', 'green', 'cyan', 'blue', 'geekblue', 'purple', '#2db7f5', '#87d068', '#108ee9', '#52c41a', '#f5222d', '#1890ff', '#faad14', '#ff0064', '#722ed1'];\nexport var getRandomColor = function () {\n  var cache = {};\n  return function (key) {\n    if (!cache[key]) {\n      var color = TAG_COLORS[Math.floor(Math.random() * TAG_COLORS.length)];\n      cache[key] = color;\n      return color;\n    }\n\n    return cache[key];\n  };\n}();","map":null,"metadata":{},"sourceType":"module"}